// // function as a parameter (callback)
// function sum(a, b){
//     return a + b;
// }
// sum (2,3);// number, string, true, false, array, object

var coffeeMachine = {
    makeCoffee: function(onFinish){
        console.log('Making coffee...');
        onFinish();
    }
};
var beep = function(){
    console.log('Tít tít')
};

coffeeMachine.makeCoffee(beep);
//onFinish được gọi là callback
coffeeMachine.makeCoffee(function(){
    console.log('Bíp bíp');
});